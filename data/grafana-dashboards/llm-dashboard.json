{
  "id": null,
  "title": "LLM Inference Server - Complete Monitoring",
  "description": "Comprehensive monitoring dashboard with all telemetry metrics",
  "tags": ["llm", "inference", "metrics", "comprehensive"],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "API Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(http_requests_total{endpoint=~\"/v1/.*\"})",
          "legendFormat": "API"
        }
      ],
      "gridPos": {"h": 8, "w": 4, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null}
            ]
          }
        }
      }
    },
    {
      "id": 31,
      "title": "Total Requests (incl. monitoring)",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(http_requests_total)",
          "legendFormat": "Total"
        }
      ],
      "gridPos": {"h": 8, "w": 4, "x": 4, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "blue", "value": null}
            ]
          }
        }
      }
    },
    {
      "id": 32,
      "title": "API Success Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(http_requests_total{endpoint=~\"/v1/.*\",status!~\"4..|5..\"}) / sum(http_requests_total{endpoint=~\"/v1/.*\"}) * 100",
          "legendFormat": "Success %"
        }
      ],
      "gridPos": {"h": 8, "w": 4, "x": 8, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Active Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "active_requests",
          "legendFormat": "Active"
        }
      ],
      "gridPos": {"h": 8, "w": 4, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Concurrent Inferences",
      "type": "stat",
      "targets": [
        {
          "expr": "concurrent_inferences",
          "legendFormat": "Running"
        }
      ],
      "gridPos": {"h": 8, "w": 4, "x": 16, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "red", "value": 5}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Models Cached",
      "type": "stat",
      "targets": [
        {
          "expr": "model_cache_size",
          "legendFormat": "Cached"
        }
      ],
      "gridPos": {"h": 8, "w": 4, "x": 20, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null}
            ]
          }
        }
      }
    },
    {
      "id": 33,
      "title": "API Request Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total{endpoint=~\"/v1/.*\"}[5m])",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "stacking": {"mode": "normal"}
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Memory & CPU Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "memory_usage_bytes{type=\"used\"}",
          "legendFormat": "Used Memory (GB)"
        },
        {
          "expr": "memory_usage_bytes{type=\"total\"}",
          "legendFormat": "Total Memory (GB)"
        },
        {
          "expr": "memory_usage_bytes{type=\"used\"} / memory_usage_bytes{type=\"total\"} * 100",
          "legendFormat": "Memory Usage %"
        },
        {
          "expr": "cpu_usage_percent",
          "legendFormat": "CPU %"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "decimals": 1,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Used Memory (GB)"},
            "properties": [
              {"id": "unit", "value": "bytes"},
              {"id": "decimals", "value": 2}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Total Memory (GB)"},
            "properties": [
              {"id": "unit", "value": "bytes"},
              {"id": "decimals", "value": 2}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Memory Usage %"},
            "properties": [
              {"id": "unit", "value": "percent"},
              {"id": "decimals", "value": 1},
              {
                "id": "color",
                "value": {
                  "mode": "thresholds"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 70},
                    {"color": "red", "value": 90}
                  ]
                }
              }
            ]
          },
          {
            "matcher": {"id": "byName", "options": "CPU %"},
            "properties": [
              {"id": "unit", "value": "percent"},
              {"id": "decimals", "value": 1},
              {
                "id": "color",
                "value": {
                  "mode": "thresholds"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 70},
                    {"color": "red", "value": 90}
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 29,
      "title": "Disk Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "disk_usage_bytes{type=\"total\"}",
          "legendFormat": "Total"
        },
        {
          "expr": "disk_usage_bytes{type=\"used\"}",
          "legendFormat": "Used"
        },
        {
          "expr": "disk_usage_bytes{type=\"free\"}",
          "legendFormat": "Free"
        },
        {
          "expr": "disk_usage_bytes{type=\"used\"} / disk_usage_bytes{type=\"total\"} * 100",
          "legendFormat": "Used %"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 1,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Used %"},
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "color",
                "value": {
                  "mode": "thresholds"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {"color": "green", "value": null},
                    {"color": "yellow", "value": 70},
                    {"color": "red", "value": 90}
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 30,
      "title": "Network I/O",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(network_io_bytes_total[5m]) / 1024",
          "legendFormat": "{{direction}} (KB/s)"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "binBps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {"mode": "normal"}
          }
        }
      }
    },
    {
      "id": 13,
      "title": "Response Time Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "P99"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 27,
      "title": "Request Size",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(request_size_bytes_bucket[5m])) / 1024",
          "legendFormat": "P50 Request (KB)"
        },
        {
          "expr": "histogram_quantile(0.95, rate(request_size_bytes_bucket[5m])) / 1024",
          "legendFormat": "P95 Request (KB)"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 28,
      "title": "Response Size",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(response_size_bytes_bucket[5m])) / 1024",
          "legendFormat": "P50 Response (KB)"
        },
        {
          "expr": "histogram_quantile(0.95, rate(response_size_bytes_bucket[5m])) / 1024",
          "legendFormat": "P95 Response (KB)"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Token Generation Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(token_generation_rate_tokens_per_second_sum[5m]) / rate(token_generation_rate_tokens_per_second_count[5m])",
          "legendFormat": "{{model_name}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "tokens/sec",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Time to First Token",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(time_to_first_token_seconds_sum[5m]) / rate(time_to_first_token_seconds_count[5m]) * 1000",
          "legendFormat": "{{model_name}} TTFT (ms)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 21,
      "title": "API Response Time Overview",
      "type": "timeseries",
      "targets": [
        {
          "expr": "api_response_time_min_seconds * 1000",
          "legendFormat": "Min - {{model_name}}"
        },
        {
          "expr": "api_response_time_max_seconds * 1000", 
          "legendFormat": "Max - {{model_name}}"
        },
        {
          "expr": "min(api_response_time_min_seconds) * 1000",
          "legendFormat": "Overall Min"
        },
        {
          "expr": "max(api_response_time_max_seconds) * 1000",
          "legendFormat": "Overall Max"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 5,
            "lineInterpolation": "stepAfter"
          },
          "color": {"mode": "palette-classic"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1000},
              {"color": "red", "value": 5000}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Overall Min"},
            "properties": [
              {"id": "custom.lineWidth", "value": 3},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Overall Max"},
            "properties": [
              {"id": "custom.lineWidth", "value": 3},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["min", "max", "mean", "lastNotNull"],
          "sortBy": "Mean",
          "sortDesc": false
        },
        "tooltip": {
          "mode": "multi",
          "sort": "asc"
        }
      }
    },
    {
      "id": 22,
      "title": "Context Utilization",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(context_utilization_ratio_bucket[5m]))",
          "legendFormat": "P50 Utilization"
        },
        {
          "expr": "histogram_quantile(0.95, rate(context_utilization_ratio_bucket[5m]))",
          "legendFormat": "P95 Utilization"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 23,
      "title": "Temperature Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(temperature_distribution_bucket[5m]))",
          "legendFormat": "P50 Temperature"
        },
        {
          "expr": "histogram_quantile(0.95, rate(temperature_distribution_bucket[5m]))",
          "legendFormat": "P95 Temperature"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 24,
      "title": "Error Rate by Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(errors_total[5m])",
          "legendFormat": "{{error_type}} - {{model_name}}"
        },
        {
          "expr": "sum by (error_type) (rate(errors_total[5m]))",
          "legendFormat": "Total {{error_type}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 15,
            "stacking": {"mode": "normal"}
          },
          "color": {"mode": "palette-classic"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.1},
              {"color": "red", "value": 1.0}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": ".*timeout.*"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": ".*memory.*|.*oom.*"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": ".*gpu.*|.*cuda.*"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "purple"}}
            ]
          }
        ]
      }
    },
    {
      "id": 25,
      "title": "Min/Max API Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "api_response_time_min_seconds * 1000",
          "legendFormat": "Min Response Time - {{model_name}}"
        },
        {
          "expr": "api_response_time_max_seconds * 1000",
          "legendFormat": "Max Response Time - {{model_name}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 56},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "lineInterpolation": "stepAfter"
          },
          "color": {"mode": "palette-classic"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1000},
              {"color": "red", "value": 5000}
            ]
          }
        }
      }
    },
    {
      "id": 16,
      "title": "Inference Queue & Load",
      "type": "timeseries",
      "targets": [
        {
          "expr": "inference_queue_depth",
          "legendFormat": "Queue Depth"
        },
        {
          "expr": "rate(inference_duration_seconds_count[5m])",
          "legendFormat": "Inference Rate/sec"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 56},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 26,
      "title": "Request Success Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total{endpoint=~\"/v1/.*\",status=~\"2..\"}[5m]) / rate(http_requests_total{endpoint=~\"/v1/.*\"}[5m]) * 100",
          "legendFormat": "Success Rate %"
        },
        {
          "expr": "rate(http_requests_total{endpoint=~\"/v1/.*\",status=~\"4..|5..\"}[5m]) / rate(http_requests_total{endpoint=~\"/v1/.*\"}[5m]) * 100",
          "legendFormat": "Error Rate %"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 64},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 90},
              {"color": "green", "value": 95}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Success Rate %"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Error Rate %"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          }
        ]
      }
    },
    {
      "id": 11,
      "title": "Token Generation Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(token_generation_rate_tokens_per_second_bucket[5m]))",
          "legendFormat": "P50 Tokens/sec"
        },
        {
          "expr": "histogram_quantile(0.95, rate(token_generation_rate_tokens_per_second_bucket[5m]))",
          "legendFormat": "P95 Tokens/sec"
        },
        {
          "expr": "rate(tokens_processed_total{type=\"completion\"}[5m])",
          "legendFormat": "Completion Tokens/sec - {{model_name}}"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 64},
      "fieldConfig": {
        "defaults": {
          "unit": "tokensps",
          "min": 0,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "color": {"mode": "palette-classic"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "green", "value": 50}
            ]
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "P50.*"},
            "properties": [
              {"id": "custom.lineWidth", "value": 3},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
            ]
          },
          {
            "matcher": {"id": "byRegexp", "options": "P95.*"},
            "properties": [
              {"id": "custom.lineWidth", "value": 2},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}
            ]
          }
        ]
      }
    },
    {
      "id": 12,
      "title": "API Key Requests",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(api_key_requests_total[5m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ],
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 64},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {"mode": "normal"}
          }
        }
      }
    },
    {
      "id": 41,
      "title": "API Key Usage Over Time",
      "type": "timeseries",

{
  "id": null,
  "title": "MLX Inference Server - Complete Monitoring",
  "description": "Comprehensive monitoring dashboard with all telemetry metrics",
  "tags": ["mlx", "inference", "metrics", "comprehensive"],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(http_requests_total)",
          "legendFormat": "Total"
        }
      ],
      "gridPos": {"h": 4, "w": 3, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Active Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "active_requests",
          "legendFormat": "Active"
        }
      ],
      "gridPos": {"h": 4, "w": 3, "x": 3, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Concurrent Inferences",
      "type": "stat",
      "targets": [
        {
          "expr": "concurrent_inferences",
          "legendFormat": "Running"
        }
      ],
      "gridPos": {"h": 4, "w": 3, "x": 6, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "red", "value": 5}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Models Cached",
      "type": "stat",
      "targets": [
        {
          "expr": "model_cache_size",
          "legendFormat": "Cached"
        }
      ],
      "gridPos": {"h": 4, "w": 3, "x": 9, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Token Generation Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(token_generation_rate_tokens_per_second_sum[5m]) / rate(token_generation_rate_tokens_per_second_count[5m])",
          "legendFormat": "{{model_name}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 5},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "tokens/sec",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Memory & CPU Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "memory_usage_bytes{type='used'} / 1024^3",
          "legendFormat": "Memory Used (GB)"
        },
        {
          "expr": "cpu_usage_percent",
          "legendFormat": "CPU %"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 5},
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Memory Used (GB)"},
            "properties": [{"id": "unit", "value": "bytes"}]
          },
          {
            "matcher": {"id": "byName", "options": "CPU %"},
            "properties": [{"id": "unit", "value": "percent"}]
          }
        ]
      }
    },
    {
      "id": 7,
      "title": "Token Processing Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(tokens_processed_total[5m])) by (type)",
          "legendFormat": "{{type}} tokens/sec"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {"mode": "normal"}
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Time to First Token",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(time_to_first_token_seconds_sum[5m]) / rate(time_to_first_token_seconds_count[5m]) * 1000",
          "legendFormat": "{{model_name}} TTFT (ms)"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Cache Operations",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(cache_operations_total[5m])",
          "legendFormat": "{{operation}}/sec"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 23},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {"mode": "normal"}
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Model Memory Footprint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "model_memory_usage_bytes / 1024^3",
          "legendFormat": "{{model_name}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 23},
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 11,
      "title": "Error Rate by Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(errors_total[5m])) by (error_type)",
          "legendFormat": "{{error_type}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 12,
      "title": "API Key Requests",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(api_key_requests_total[5m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {"mode": "normal"}
          }
        }
      }
    },
    {
      "id": 13,
      "title": "Response Time Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m])) * 1000",
          "legendFormat": "P99"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 41},
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 14,
      "title": "Token Usage per Request",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(tokens_per_request_sum[5m]) / rate(tokens_per_request_count[5m])",
          "legendFormat": "Avg {{type}} tokens/request"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 41},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 15,
      "title": "API Key Token Usage (Total)",
      "type": "table",
      "targets": [
        {
          "expr": "sum(api_key_token_usage_total) by (api_key_prefix, type)",
          "legendFormat": "{{api_key_prefix}} - {{type}}",
          "format": "table",
          "instant": true
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 50},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "displayMode": "table"
          }
        }
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true
            }
          }
        }
      ]
    },
    {
      "id": 16,
      "title": "Inference Queue & Load",
      "type": "timeseries",
      "targets": [
        {
          "expr": "inference_queue_depth",
          "legendFormat": "Queue Depth"
        },
        {
          "expr": "rate(inference_duration_seconds_count[5m])",
          "legendFormat": "Inference Rate/sec"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 50},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "schemaVersion": 30,
  "version": 1,
  "links": []
}